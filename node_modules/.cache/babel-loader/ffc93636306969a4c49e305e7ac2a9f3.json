{"ast":null,"code":"import product from './SingleProductComponent.vue';\nexport default {\n  name: 'ProductComponent',\n  components: {\n    product\n  },\n\n  data() {\n    return {\n      catalogUrl: '/catalogData.json',\n      filtered: [{\n        id_product: 1,\n        product_name: \"ELLERY X M'O CAPSULE_001\",\n        price: 1600,\n        img_product: \"images/featured/1.jpg\"\n      }, {\n        id_product: 2,\n        product_name: \"ELLERY X M'O CAPSULE_002\",\n        price: 3000,\n        img_product: \"images/featured/2.jpg\"\n      }],\n      products: [],\n      imgProduct: ''\n    };\n  },\n\n  mounted() {\n    this.$root.getJson(`/api/products`).then(data => {\n      for (let item of data) {\n        item.imgPath = `../images/featured/${item.id_product}.jpg`;\n        this.$data.products.push(item);\n        this.$data.filtered.push(item);\n      }\n    });\n  },\n\n  methods: {\n    filter(userSearch) {\n      let regexp = new RegExp(userSearch, 'i');\n      this.filtered = this.products.filter(el => regexp.test(el.product_name));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaA;AAEA;EACAA,wBADA;EAEAC;IACAC;EADA,CAFA;;EAMAC;IACA;MACAC,+BADA;MAEAC,WACA;QACAC,aADA;QAEAC,wCAFA;QAGAC,WAHA;QAIAC;MAJA,CADA,EAOA;QACAH,aADA;QAEAC,wCAFA;QAGAC,WAHA;QAIAC;MAJA,CAPA,CAFA;MAgBAC,YAhBA;MAiBAC;IAjBA;EAmBA,CA1BA;;EA2BAC;IACA,oCACAC,IADA,CACAV;MACA;QACAW;QACA;QACA;MACA;IACA,CAPA;EASA,CArCA;;EAsCAC;IACAC;MACA;MACA;IACA;;EAJA;AAtCA","names":["name","components","product","data","catalogUrl","filtered","id_product","product_name","price","img_product","products","imgProduct","mounted","then","item","methods","filter"],"sourceRoot":"src/components","sources":["ProductComponent.vue"],"sourcesContent":["<template>\n    <div class=\"featuredItems\">\n        <product v-for=\"item of filtered\"\n            :key=\"item.id_product\" \n            :img=\"item.imgPath\"\n            :product=\"item\"\n            @add-product=\"$parent.$refs.headercomp.$refs.cart.addProduct\">\n        </product>\n    </div>\n</template>\n\n\n<script>\n    import product from './SingleProductComponent.vue'\n\n    export default {\n    name: 'ProductComponent',\n    components: {\n        product\n    },\n\n    data() {\n        return {\n            catalogUrl: '/catalogData.json',\n            filtered: [\n                {\n                id_product: 1,\n                product_name: \"ELLERY X M'O CAPSULE_001\",\n                price: 1600,\n                img_product: \"images/featured/1.jpg\"\n            },\n            {\n                id_product: 2,\n                product_name: \"ELLERY X M'O CAPSULE_002\",\n                price: 3000,\n                img_product: \"images/featured/2.jpg\"\n            },\n            ],\n            products: [],\n            imgProduct: ''\n        }\n    },\n        mounted() {\n            this.$root.getJson(`/api/products`)\n                .then(data => {\n                    for (let item of data){\n                        item.imgPath = `../images/featured/${item.id_product}.jpg`;\n                        this.$data.products.push(item);\n                        this.$data.filtered.push(item);\n                    }\n                });\n\n        },\n        methods: {\n            filter(userSearch){\n                let regexp = new RegExp(userSearch, 'i');\n                this.filtered = this.products.filter(el => regexp.test(el.product_name));\n            }\n        }\n    }\n</script>"]},"metadata":{},"sourceType":"module"}